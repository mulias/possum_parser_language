crate = "[" > char < "]"
no_crate = "   " $ null
cargo = crate | no_crate

move =
  "move" & ws & number  -> Count & ws &
  "from" & ws & numeral -> From  & ws &
  "to"   & ws & numeral -> To    & ws $
  {"count": Count, "from": From, "to": To}

table_sep(cargo, " ", nl) -> CargoRows &
ws > array_sep(numeral, ws) < ws -> _Labels &
array(move) -> Steps $
{"cargo_rows": CargoRows, "steps": Steps}
